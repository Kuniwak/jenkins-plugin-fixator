#!/bin/bash -eu
set -o pipefail
set -x


function print-jenkins-plugin-hpi-url() {
  local plugin_name plugin_version
  plugin_name="$1"
  plugin_version="$2"

  echo "http://updates.jenkins-ci.org/download/plugins/$plugin_name/$plugin_version/$plugin_name.hpi"
}


print-jenkins-plugin-hpi-url "$@"
